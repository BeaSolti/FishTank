package com.fishtank;

import java.util.ArrayList;

public class Main{
    public static void main(String args[]){
        ArrayList<Fish> Fishes = new ArrayList<Fish>();
        //constructor is created under Fish class
        Fish clownfish = new Fish("Dory","Clownfish", 15, "blue and yellow",false);
        Fishes.add(clownfish);
        Fish tag = new Fish("Nemo","Tag",20, "grey",true);
        Fishes.add(tag);
        Fish kong = new Fish("Gill","Kong",25, "grey",true);
        Fishes.add(kong);
    }

}
